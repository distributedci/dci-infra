[project]
	description = Bunch of ansible modules to manage dci public infra
[access "refs/*"]
  read = group dci-infra-core
  owner = group dci-infra-ptl
[access "refs/heads/*"]
	label-Code-Review = -2..+2 group dci-infra-core
	label-Verified = -2..+2 group dci-infra-ptl
	label-Workflow = -1..+1 group dci-infra-core
	submit = group dci-infra-ptl
	read = group dci-infra-core
    read = group Registered Users
[access "refs/meta/config"]
    read = group dci-infra-core
    read = group Registered Users
[receive]
	requireChangeId = true
[submit]
	mergeContent = false
	action = fast forward only
